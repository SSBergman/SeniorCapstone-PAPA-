@model List<ASP_Test.Models.Student>
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860

@{
}
<h1>Students</h1>
<table class="table">
    <thead>
        <tr>
            <th>ID </th>
            <th>Email </th>
           
        </tr>
    <tbody>
        @foreach(var student in Model)
            {
                <tr>
                <td>@student.StudentId </td>
                <td>@student.Email </td>
             
                </tr>
            }
        </tbody>
</table>